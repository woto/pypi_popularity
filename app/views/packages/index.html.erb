<h1>Listing packages</h1>

<br/>

<%= will_paginate @packages %>

<br />

<table>
  <tr>
    <th style="width: 700px">Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>


<% @packages.each do |package| %>
  <tr>
    <td><%= package.title %></td>
    <td><%= link_to 'Show', package_package_histories_path(package) %></td>
    <td><%= link_to 'Edit', edit_package_path(package) %></td>
    <td><%= link_to 'Destroy', package, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= will_paginate @packages %>

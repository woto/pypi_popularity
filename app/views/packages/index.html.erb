<h1>Listing packages</h1>

<br/>

<%= will_paginate @packages %>

<br />

<table>
  <tr>
    <th style="width: 400px">Title</th>
    <th>Description</th>
  </tr>


<% @packages.each do |package| %>
  <tr>
    <td><%= link_to package.title, package_package_histories_path(package) %></td>
    <td><%= package.description %></td>
  </tr>
<% end %>
</table>

<br />

<%= will_paginate @packages %>

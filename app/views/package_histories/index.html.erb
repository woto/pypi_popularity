<h1>Listing package_histories</h1>

<table>
  <tr>
    <th>Package</th>
    <th>Date</th>
    <th>Downloads</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @package_histories.each do |package_history| %>
  <tr>
    <td><%= package_history.package %></td>
    <td><%= package_history.date %></td>
    <td><%= package_history.downloads %></td>
    <td><%= link_to 'Show', package_history %></td>
    <td><%= link_to 'Edit', edit_package_history_path(package_history) %></td>
    <td><%= link_to 'Destroy', package_history, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Package history', new_package_history_path %>
